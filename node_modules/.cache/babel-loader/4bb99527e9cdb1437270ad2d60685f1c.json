AAAA;AAAA;AAAA;AASnB,KA7GM;AA8GP,IAAA,MA9GO,oBA8GE;AACL,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,QAAjB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,YAAjB;AACH,KAjHM;AAkHP,IAAA,oBAlHO,gCAkHc,GAlHd,EAkHmB;AACxB,WAAK,YAAL,GAAoB,GAAE,GAAI,KAAK,YAAT,GAAwB,EAA9C;AACA,WAAK,eAAL,GAAuB,KAAvB;AACD,KArHM;AAsHP,IAAA,yBAtHO,qCAsHmB,KAtHnB,EAsH0B;AAC/B,UAAI,YAAW,GAAI,KAAK,CAAC,MAAzB;AACA,WAAK,QAAL,GAAgB,YAAW,KAAM,KAAK,MAAL,CAAY,MAA7C;AACA,WAAK,eAAL,GACE,YAAW,GAAI,CAAf,IAAoB,YAAW,GAAI,KAAK,MAAL,CAAY,MADjD;AAED;AA3HM,GA/CI;AA4Kb,EAAA,OA5Ka,qBA4KH;AACR;AACA,IAAA,OAAO,CAAC,GAAR,CAAY,QAAZ,EAFQ,CAGR;AACD,GAhLY;AAiLb,EAAA,OAjLa,qBAiLH;AACR,SAAK,OAAL,CAAa,CAAb;AACD;AAnLY,CAAf","sourcesContent":["<template>\r\n  <span class=\"lab\">时间范围</span>\r\n  <el-date-picker\r\n    v-model=\"time\"\r\n    type=\"datetimerange\"\r\n    :shortcuts=\"shortcuts\"\r\n    :default-time=\"defaultTime\"\r\n    range-separator=\"至\"\r\n    start-placeholder=\"开始日期\"\r\n    end-placeholder=\"结束日期\"\r\n  >\r\n  </el-date-picker>\r\n  <el-button @click=\"dialogVisible = true\">选择点位</el-button>\r\n  <el-dialog\r\n    title=\"选择查询点位\"\r\n    v-model=\"dialogVisible\"\r\n    width=\"20%\"\r\n    :before-close=\"handleClose\"\r\n  >\r\n    <div>\r\n      <el-select v-model=\"wellname\" @change=\"change\" placeholder=\"请选择设备名称\">\r\n        <el-option\r\n          v-for=\"item in select\"\r\n          :key=\"item\"\r\n          :label=\"item\"\r\n          :value=\"item\"\r\n        >\r\n        </el-option>\r\n      </el-select>\r\n    </div>\r\n    <el-checkbox\r\n      :indeterminate=\"isIndeterminate\"\r\n      v-model=\"checkAll\"\r\n      @change=\"handleCheckAllChange\"\r\n      >全选</el-checkbox\r\n    >\r\n    <div style=\"margin: 15px 0\"></div>\r\n    <el-checkbox-group\r\n      v-model=\"checkedPoint\"\r\n      @change=\"handleCheckedPointsChange\"\r\n    >\r\n      <el-checkbox v-for=\"point in points\" :label=\"point\" :key=\"point\">{{\r\n        point\r\n      }}</el-checkbox>\r\n    </el-checkbox-group>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n  <el-button type=\"primary\" @click=\"queryMethos\">查询数据</el-button>\r\n  <el-divider></el-divider>\r\n\r\n  <div id=\"container\"></div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      request: {\r\n        StartTime: \"\",\r\n        StopTime: \"\",\r\n        WellName: \"\",\r\n        Point: [],\r\n      },\r\n      dialogVisible: false,\r\n      wellname: \"\",\r\n      checkAll: false,\r\n      checkedPoint: [],\r\n      pointOptions: [\"温度\", \"偏移量\", \"报警\"],\r\n      points: [\"温度\", \"偏移量\", \"报警\"],\r\n      isIndeterminate: true,\r\n      select: [1, 2, 3, 4, 5],\r\n      options: {},\r\n      shortcuts: [\r\n        {\r\n          text: \"今天\",\r\n          value: new Date(),\r\n        },\r\n        {\r\n          text: \"昨天\",\r\n          value: () => {\r\n            const date = new Date();\r\n            date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n            return date;\r\n          },\r\n        },\r\n        {\r\n          text: \"一周前\",\r\n          value: () => {\r\n            const date = new Date();\r\n            date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n            return date;\r\n          },\r\n        },\r\n      ],\r\n      time: '',\r\n      defaultTime: [\r\n          new Date(2000, 1, 1, 0, 0, 0),\r\n          new Date(2000, 2, 1, 23, 59, 59)\r\n        ] // '00:00:00', '23:59:59'\r\n    };\r\n  },\r\n  methods: {\r\n    Mychart(num) {\r\n      var mychart = this.$echarts.init(document.getElementById(\"container\"));\r\n      var option = {\r\n        tooltip: {\r\n          trigger: \"axis\",\r\n          position: function (pt) {\r\n            return [pt[0], \"10%\"];\r\n          },\r\n        },\r\n        title: {\r\n          left: \"center\",\r\n          text: \"大数据量面积图\",\r\n        },\r\n        toolbox: {\r\n          feature: {\r\n            dataZoom: {\r\n              yAxisIndex: \"none\",\r\n            },\r\n            restore: {},\r\n            saveAsImage: {},\r\n          },\r\n        },\r\n        xAxis: {\r\n          type: \"category\",\r\n          boundaryGap: false,\r\n          data: [],\r\n        },\r\n        yAxis: {\r\n          type: \"value\",\r\n          boundaryGap: [0, \"100%\"],\r\n        },\r\n        dataZoom: [\r\n          {\r\n            type: \"inside\",\r\n            start: 0,\r\n            end: 100,\r\n          },\r\n          {\r\n            start: 0,\r\n            end: 100,\r\n          },\r\n        ],\r\n        series: [],\r\n      };\r\n      console.log(this.options);\r\n      if (num == 1) {\r\n        for (var i = 0; i < this.options.xAxis.length; i++) {\r\n          var s = new Object();\r\n          s.serie = {\r\n            name: \"\",\r\n            type: \"line\",\r\n            symbol: \"none\",\r\n            sampling: \"lttb\",\r\n            itemStyle: {\r\n              color: \"\",\r\n            },\r\n            areaStyle: {\r\n              color: {},\r\n            },\r\n            data: [],\r\n          };\r\n          s.series.itemStyle.color = this.options.series[i].borderColor;\r\n          s.series.areaStyle.color = new this.$echarts.graphic.LinearGradient(\r\n            0,\r\n            0,\r\n            0,\r\n            1,\r\n            [\r\n              {\r\n                offset: 0,\r\n                color: this.options.series[i].Color,\r\n              },\r\n              {\r\n                offset: 1,\r\n                color: this.options.series[i].borderColor,\r\n              },\r\n            ]\r\n          );\r\n          s.series.name = this.options.series[i].name;\r\n          s.series.data = this.options.series[i].data;\r\n        }\r\n        option.series.push(s.serie);\r\n      }\r\n      if (option && typeof option === \"object\") {\r\n        mychart.setOption(option);\r\n      }\r\n    },\r\n    async getChartsData() {\r\n      this.$refs.loginFormRef.validate(async (valid) => {\r\n        if (!valid) return;\r\n        const { data: res } = await this.$http.get(\r\n          \"Well/GetHistoryWellDatas\",\r\n          this.request\r\n        );\r\n        if (res.meta.status !== 200)\r\n          return this.$message.error(\"获取数据失败！\");\r\n        this.options = res.option;\r\n      });\r\n    },\r\n    async queryMethos() {\r\n      if (this.time == \"\") return this.$message.error('时间不能为空！')     \r\n      this.request.StartTime = this.time[0];\r\n      this.request.StopTime = this.time[1];\r\n      this.request.WellName = this.wellname;\r\n      this.request.Point = this.checkedPoint;\r\n      console.log(this.request)\r\n      const { data: res } = await this.$http.get('/hhhh',this.request)\r\n      console.log(res.data)\r\n    },\r\n    change() {\r\n        console.log(this.wellname);\r\n        console.log(this.checkedPoint)\r\n    },\r\n    handleCheckAllChange(val) {\r\n      this.checkedPoint = val ? this.pointOptions : [];\r\n      this.isIndeterminate = false;\r\n    },\r\n    handleCheckedPointsChange(value) {\r\n      let checkedCount = value.length;\r\n      this.checkAll = checkedCount === this.points.length;\r\n      this.isIndeterminate =\r\n        checkedCount > 0 && checkedCount < this.points.length;\r\n    },\r\n  },\r\n  created() {\r\n    //获取数据\r\n    console.log(\"开始获取数据\");\r\n    //this.getChartsData();\r\n  },\r\n  mounted() {\r\n    this.Mychart(0);\r\n  },\r\n};\r\n</script>\r\n}\r\n<style lang=\"less\" scoped>\r\n#container {\r\n  height: calc(100% - 100px);\r\n  width: 100%;\r\n}\r\n.lab {\r\n  display: inline-block;\r\n  margin-left: 50px;\r\n  margin-right: 50px;\r\n  font-family: \"Helvetica Neue\";\r\n  color: #606266;\r\n}\r\n.el-button {\r\n  margin-left: 20px;\r\n  margin-right: 20px;\r\n}\r\n.el-select {\r\n  margin-bottom: 10px;\r\n}\r\n</style>"],"sourceRoot":""}]}