,SAAS,EAAEL;AAA9B,GAJQ,EAKR;AAAEG,IAAAA,IAAI,EAAE,aAAR;AAAuBE,IAAAA,SAAS,EAAEJ,UAAlC;AAA6CM,IAAAA,IAAI,EAAC;AAACC,MAAAA,SAAS,EAAE;AAAZ;AAAlD,GALQ;AAJZ,CAHa,CAAf;AAiBA,IAAMC,MAAM,GAAGhB,YAAY,CAAC;AAC1BiB,EAAAA,OAAO,EAAEhB,oBAAoB,EADH;AAE1BQ,EAAAA,MAAM,EAANA;AAF0B,CAAD,CAA3B,C,CAIA;AACA;;AACAO,MAAM,CAACE,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC;AACA;AACA;AACA;AACA,MAAIF,EAAE,CAACT,IAAH,KAAY,QAAhB,EAA0B,OAAOW,IAAI,EAAX,CALU,CAMpC;;AACA,MAAMC,QAAQ,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,OAA9B,CAAjB;AACA,MAAI,CAACH,QAAL,EAAe,OAAOD,IAAI,CAAC,QAAD,CAAX;AACfA,EAAAA,IAAI;AACL,CAVD,E,CAWA;;AACA,eAAeL,MAAf","sourcesContent":["import { createRouter, createWebHashHistory } from 'vue-router'\nimport Home from '../views/Home.vue'\nimport Login from '../views/Login.vue'\nimport Cockpit from '../components/Cockpit.vue' \nimport User from '../components/User.vue'\nimport WellData from '../components/WellData.vue'\nimport Device from '../components/DeviceManagement/Device.vue'\nimport DataCharts from '../components/DeviceManagement/DataCharts.vue'\n\nconst routes = [\n  { path: '/', redirect: '/login' },\n  { path: '/login', component: Login },\n  {\n    path: '/home',\n    component: Home,\n    redirect: '/cockpit',\n    children: [\n      { path: '/cockpit', component: Cockpit },\n      { path: '/user', component: User },\n      { path: '/welldata', component: WellData },\n      { path: '/device', component: Device },\n      { path: '/datacharts', component: DataCharts,meta:{keepAlive: false} }\n    ]\n  }\n]\n\nconst router = createRouter({\n  history: createWebHashHistory(),\n  routes\n})\n//*\n// 挂载路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问的路径\n  // from 代表从哪个路径跳转而来\n  // next 是一个函数，表示放行\n  //     next()  放行    next('/login')  强制跳转\n  if (to.path === '/login') return next()\n  // 获取token\n  const tokenStr = window.sessionStorage.getItem('token')\n  if (!tokenStr) return next('/login')\n  next()\n})\n//*/\nexport default router\n"]}]}