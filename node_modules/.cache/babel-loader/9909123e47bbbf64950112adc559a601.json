7DN,E,gBACE,YAAA,CAuDM,KAvDN,EAuDM;AAvDD,IAAA,KAAK,EAAC,yBAuDL;AAvDgC,IAAA,KAAK,EAAE,IAAA,CAAA,KAuDvC;AAvD+C,IAAA,MAAM,EAAE,IAAA,CAAA;AAuDvD,GAvDN,E,CACE,YAAA,CA8BO,MA9BP,EA8BO,IA9BP,EA8BO,CA7BL,YAAA,CAIE,MAJF,EAIE;AAHC,IAAA,EAAE,EAAE,KAAA,CAAA,IAGL;AAFC,IAAA,CAAC,EAAE,QAAA,CAAA,KAEJ;AADA,IAAA,IAAI,EAAC;AACL,GAJF,E,IAAA,E;;AAAA,I,WAAA,CA6BK,EAxBL,YAAA,CAYiB,gBAZjB,EAYiB;AAXd,IAAA,EAAE,EAAE,KAAA,CAAA,QAWU;AAVf,IAAA,EAAE,EAAC,KAUY;AAVN,IAAA,EAAE,EAAC,KAUG;AAVG,IAAA,CAAC,EAAC;AAUL,GAZjB,E,CAIE,U,EAIA,U,CARF,E;;AAAA,I,MAAA,CAwBK,EAVL,YAAA,CASO,MATP,EASO;AATA,IAAA,EAAE,EAAE,KAAA,CAAA;AASJ,GATP,EAAe,CACb,YAAA,CAOS,QAPT,EAOS;AAPD,IAAA,EAAE,EAAC,GAOF;AAPM,IAAA,EAAE,EAAC,GAOT;AAPa,IAAA,CAAC,EAAC,KAOf;AAPsB,IAAA,IAAI,iBAAU,KAAA,CAAA,QAAV;AAO1B,GAPT,E,CACE,YAAA,CAKE,eALF,EAKE;AAJC,IAAA,GAAG,YAAK,MAAA,CAAA,GAAL,MAIJ;AAHC,IAAA,IAAI,EAAE,QAAA,CAAA,KAGP;AAFA,IAAA,MAAM,EAAC,MAEP;AADA,IAAA,WAAW,EAAC;AACZ,GALF,E,IAAA,E;;AAAA,I,eAAA,C,CADF,E;;AAAA,I,QAAA,CADa,CAAf,E;;AAAA,I,MAAA,CAUK,CA9BP,C,EAgCA,YAAA,CAAgH,SAAhH,EAAgH;AAAtG,IAAA,IAAI,EAAE,MAAA,CAAA,eAAgG;AAA9E,IAAA,MAAM,iBAAU,IAAA,CAAA,KAAA,GAAK,CAAf,iBAA0B,IAAA,CAAA,KAAA,GAAK,CAA/B,cAAuC,IAAA,CAAA,MAAA,GAAM,CAA7C,iBAAwD,IAAA,CAAA,MAAA,GAAM,CAA9D;AAAwE,GAAhH,E,IAAA,E;;AAAA,I,kBAAA,C,EAEA,YAAA,CAIE,KAJF,EAIE;AAHC,IAAA,MAAM,EAAE,KAAA,CAAA,WAAA,CAAW,CAAX,CAGT;AAFA,oBAAa,GAEb;AADC,6BAAgB,KAAA,CAAA,IAAhB;AACD,GAJF,E,IAAA,E;;AAAA,I,wBAAA,C,EAMA,YAAA,CAaM,KAbN,EAaM;AAZH,IAAA,MAAM,EAAE,KAAA,CAAA,WAAA,CAAW,CAAX,CAYL;AAXJ,oBAAa,GAWT;AAVH,6BAAgB,KAAA,CAAA,IAAhB,CAUG;AATH,IAAA,IAAI,iBAAU,KAAA,CAAA,IAAV;AASD,GAbN,E,CAME,YAAA,CAME,SANF,EAME;AALA,IAAA,aAAa,EAAC,kBAKd;AAJC,IAAA,IAAI,eAAQ,QAAA,CAAA,MAAR,CAIL;AAHC,IAAA,EAAE,YAAK,QAAA,CAAA,MAAL,QAGH;AAFC,IAAA,GAAG,YAAK,MAAA,CAAA,GAAL,MAEJ;AADA,IAAA,WAAW,EAAC;AACZ,GANF,E,IAAA,E;;AAAA,I,qBAAA,C,CANF,E;;AAAA,I,gCAAA,C,CAzCF,E;;AAAA,I,mBAAA,C,GAyDA,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,WAAA,CAAa,IAAA,CAAA,MAAb,EAAa,SAAb,CACI,CAFN,C,CA1DF,E;;AAAA,G","sourcesContent":["<template>\n  <div class=\"dv-border-box-8\" :ref=\"ref\">\n    <svg class=\"dv-border-svg-container\" :width=\"width\" :height=\"height\">\n      <defs>\n        <path\n          :id=\"path\"\n          :d=\"pathD\"\n          fill=\"transparent\"\n        />\n        <radialGradient\n          :id=\"gradient\"\n          cx=\"50%\" cy=\"50%\" r=\"50%\"\n        >\n          <stop\n            offset=\"0%\" stop-color=\"#fff\"\n            stop-opacity=\"1\"\n          />\n          <stop\n            offset=\"100%\" stop-color=\"#fff\"\n            stop-opacity=\"0\"\n          />\n        </radialGradient>\n\n        <mask :id=\"mask\">\n          <circle cx=\"0\" cy=\"0\" r=\"150\" :fill=\"`url(#${gradient})`\">\n            <animateMotion\n              :dur=\"`${dur}s`\"\n              :path=\"pathD\"\n              rotate=\"auto\"\n              repeatCount=\"indefinite\"\n            />\n          </circle>\n        </mask>\n      </defs>\n\n      <polygon :fill=\"backgroundColor\" :points=\"`5, 5 ${width - 5}, 5 ${width - 5} ${height - 5} 5, ${height - 5}`\" />\n\n      <use\n        :stroke=\"mergedColor[0]\"\n        stroke-width=\"1\"\n        :xlink:href=\"`#${path}`\"\n      />\n\n      <use\n        :stroke=\"mergedColor[1]\"\n        stroke-width=\"3\"\n        :xlink:href=\"`#${path}`\"\n        :mask=\"`url(#${mask})`\"\n      >\n        <animate\n          attributeName=\"stroke-dasharray\"\n          :from=\"`0, ${length}`\"\n          :to=\"`${length}, 0`\"\n          :dur=\"`${dur}s`\"\n          repeatCount=\"indefinite\"\n        />\n      </use>\n    </svg>\n\n    <div class=\"border-box-content\">\n      <slot></slot>\n    </div>\n  </div>\n</template>\n\n<script>\nimport autoResize from '../../../mixin/autoResize'\nimport { uuid } from '../../../util/index'\n\nimport { deepMerge } from '@jiaminghi/charts/lib/util/index'\n\nimport { deepClone } from '@jiaminghi/c-render/lib/plugin/util'\n\nexport default {\n  name: 'DvBorderBox8',\n  mixins: [autoResize],\n  props: {\n    color: {\n      type: Array,\n      default: () => ([])\n    },\n    dur: {\n      type: Number,\n      default: 3\n    },\n    backgroundColor: {\n      type: String,\n      default: 'transparent'\n    },\n    reverse: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data () {\n    const id = uuid()\n    return {\n      ref: 'border-box-8',\n      path: `border-box-8-path-${id}`,\n      gradient: `border-box-8-gradient-${id}`,\n      mask: `border-box-8-mask-${id}`,\n\n      defaultColor: ['#235fa7', '#4fd2dd'],\n\n      mergedColor: []\n    }\n  },\n  computed: {\n    length () {\n      const { width, height } = this\n\n      return (width + height - 5) * 2\n    },\n    pathD () {\n      const { reverse, width, height } = this\n\n      if (reverse) return `M 2.5, 2.5 L 2.5, ${height - 2.5} L ${width - 2.5}, ${height - 2.5} L ${width - 2.5}, 2.5 L 2.5, 2.5`\n\n      return `M2.5, 2.5 L${width - 2.5}, 2.5 L${width - 2.5}, ${height - 2.5} L2.5, ${height - 2.5} L2.5, 2.5`\n    }\n  },\n  watch: {\n    color () {\n      const { mergeColor } = this\n\n      mergeColor()\n    }\n  },\n  methods: {\n    mergeColor () {\n      const { color, defaultColor } = this\n\n      this.mergedColor = deepMerge(deepClone(defaultColor, true), color || [])\n    }\n  },\n  mounted () {\n    const { mergeColor } = this\n\n    mergeColor()\n  }\n}\n</script>"],"sourceRoot":""}]}